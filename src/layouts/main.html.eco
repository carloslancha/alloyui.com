<!DOCTYPE html>
<html lang="en">
<head>
    <title>AlloyUI</title>

    <meta charset="utf-8">
    <meta name="author" content="Eduardo Lundgren, Nate Cavanaugh">
    <meta name="description" content="<%= @getPreparedDescription() %>">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Facebook Tags -->
    <meta property="fb:app_id" content="176947299114210">
    <meta property="og:type" content="website">
    <meta property="og:url" content="<%= @getSiteUrl() %>">
    <meta property="og:image" content="<%= @getAssetsUrl() %>/img/facebook-share.jpg">
    <meta property="og:title" content="<%= @getPreparedTitle() %>">
    <meta property="og:description" content="<%= @getPreparedDescription() %>">

    <!-- Favicon -->
    <link rel="shortcut icon" href="<%= @getAssetsUrl() %>/img/favicon.ico" type="image/x-icon">

    <!-- Fonts -->
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Dosis|Roboto:400,700,700italic,400italic">

    <!-- CSS -->
    <link rel="stylesheet" href="<%= @getBootstrapCSS() %>">
    <link rel="stylesheet" href="<%= @getAssetsUrl() %>/css/main.css">
    <link rel="stylesheet" href="<%= @getAssetsUrl() %>/css/navbar.css">
    <link rel="stylesheet" href="<%= @getAssetsUrl() %>/css/page.css">
    <link rel="stylesheet" href="<%= @getAssetsUrl() %>/css/examples.css">
    <link rel="stylesheet" href="<%= @getAssetsUrl() %>/css/code.css">
    <link rel="stylesheet" href="<%= @getAssetsUrl() %>/css/home.css">
    <link rel="stylesheet" href="<%= @getAssetsUrl() %>/css/footer.css">

    <!-- JS -->
    <script src="<%= @getCdnSeed() %>"></script>
</head>
<body>
    <%- @content %>

    <%- @partial('footer') %>

    <script src="<%= @getAssetsUrl() %>/js/navbar.js"></script>
</body>
</html>
